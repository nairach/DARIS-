		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script><script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>

		<style>
h1{
	font-weight: normal;
	font-size: 30px;
}

h2{
	font-weight: normal;
	font-size: 30px;
}
		.well{
			background-color: pink;
		}

		</style>
	<script>

	$.getJSON( "https://www.reddit.com/r/politics/top.json?limit=10&t=hour", function(data){
	 $.each(data.data.children.slice(0,10), function(i, post){
	 	$("#reddit-content").append('<br>' + post.data.title +'<br>');
	 });
});

$.getJSON( "https://www.reddit.com/r/politics/top.json?limit=10&t=day", function(data){
	 $.each(data.data.children.slice(0,10), function(i, post){
	 	$("#day").append('<br>' +  post.data.title   +'<br>');
	 });
});

	</script>
	</head>

